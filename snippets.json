{
    "for": {
        "prefix": "for",
        "body": "for ${1:item} in ${2:iterable}:\n\t${3:pass}",
        "description": "For Loop"
    },
    "def": {
        "prefix": "def",
        "body": "def ${1:function_name}(${2:args}):\n\t\"\"\"${3:docstring}\"\"\"\n\t${4:pass}",
        "description": "Function Definition"
    },
    "class": {
        "prefix": "class",
        "body": "class ${1:ClassName}(${2:object}):\n\t\"\"\"${3:docstring}\"\"\"\n\tdef __init__(self, ${4:args}):\n\t\t${5:pass}",
        "description": "Class Definition"
    },
    "if": {
        "prefix": "if",
        "body": "if ${1:condition}:\n\t${2:pass}",
        "description": "If Statement"
    },
    "main": {
        "prefix": "main",
        "body": "if __name__ == \"__main__\":\n\t${1:pass}",
        "description": "Main execution block"
    }
}
